<div>
	<%= react_component(('BannerPictures'), {prerender: true}) %>
</div>

<div class="home-page">
	<div class="first-box">
		<%= react_component(('FirstBoxInformation'), {prerender: true}) %>
	</div>
	<div class="second-box">
		<div class="container">
			<%= react_component(('InformationBoxes'), {prerender: true}) %>
		</div>
	</div>
	<div class="third-box">
		<div class="container">
			<div class="row">
				<div class="col-lg-12">
					<h2 class="home-title">
						<p class="typewrite" data-period="2000" data-type='["Checkout Our Inventory"]'>
						<span class="wrap"></span></p></h2>
					</div>
						<%@inventories.each do |img|%>
					<div id="mobile-pic" class="col-md-2 "style="width: 16.9rem">
						<ul>
							<li class="img-responsive border-image"><%=image_tag(img.photos.first.image(:medium))%></li>
						</ul>
				 </div>
				<%end%>
			</div>
		</div>
	</div>
</div>
